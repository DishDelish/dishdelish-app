{
  "rules": {
    "recipes": {
      ".indexOn": ["recipeName", "userName", "cookTime", "prepTime", "likes"]
    },
    "favorites": {
      "$user_id": {
      	".indexOn": [".value"],
          ".write": "$user_id === auth.uid"
      }
    },
    ".read": true,  //Allow read and write access to anyone 
    ".write": true,  //Not recommended for release production 
  }
}
